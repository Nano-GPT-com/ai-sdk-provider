{
  "name": "@nanogpt/ai-sdk-provider",
  "version": "0.1.1",
  "description": "NanoGPT community provider for the Vercel AI SDK.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "release": "node ./scripts/release.mjs"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nano-GPT-com/ai-sdk-provider"
  },
  "bugs": {
    "url": "https://github.com/Nano-GPT-com/ai-sdk-provider/issues"
  },
  "keywords": [
    "vercel-ai",
    "ai-sdk",
    "nanogpt",
    "provider",
    "llm"
  ],
  "dependencies": {
    "vercel": "^48.1.4",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "ai": "^5.0.0"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.1",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "ai": "^5.0.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "tsd": "^0.31.1",
    "tslib": "^2.6.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  }
}
